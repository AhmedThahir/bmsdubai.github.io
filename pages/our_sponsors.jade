extends _layout.jade

block content
	.sponsors
		.header
			h1 Our Sponsors
		.title_sponsor
			h3 Title Sponsor
			a(href="https://mahykhoory.com/", target="_blank")
				img(data='logo/khoory_light.svg', alt="Mahy Khoory Logo")
		.technical_sponsors(style="width: 100%; margin:auto")
			h3 Technical Sponsors
			.grid
				a(href="https://www.instagram.com/nrt_performance/", target="_blank")
					img(data='logo/nrt.png')
				img(data='logo/titan_white.svg', alt="Titan Performance Logo")
		.sponsors
			h3 Commercial Sponsors
			.grid
				a(href="https://www.bits-pilani.ac.in/Dubai/", target="_blank")
					img(data='logo/bitsLogo.svg', alt="BITS Pilani Dubai Campus Logo")
				a(href="https://www.societymotors.ae/en", target="_blank")
					img(data='logo/SocietyMotors.png', alt="Society Motors Logo")
				a(href="https://alshirawiequipment.com/", target="_blank")
					img(data='logo/al_shirawi.png', style = "background: white; padding: 1em")
				a(href="https://www.paragonrestaurant.net/", target="_blank")
					img(data='logo/paragon.jpg', alt="Calicut Paragon Logo")
				a(href="https://www.af1racing.com/", target="_blank")
					img(data='logo/af1_racing.png', alt="AF1 Racing Logo")